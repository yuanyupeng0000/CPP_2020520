name: "batchNorm"
layer {
  name: "data"
  type: "Input"
  top: "data"
  input_param { shape: { dim: 19 dim: 37 } }
}

layer {
  name: "bn"
  type: "BatchNorm"
  top: "bn"
  bottom: "data"
  param {lr_mult: 0}
  param {lr_mult: 0}
  param {lr_mult: 0}

  batch_norm_param {
    use_global_stats: true
    moving_average_fraction: 0.9
    eps: 0.0001
  }
}
