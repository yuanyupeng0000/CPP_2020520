{
    "model": "mobilenetv2",
    "input_sample_size": [1, 3, 224, 224],
    "num_classes": 1000,
    "batch_size" : 256,
    "weights": "/home/alex_k/work/algo/nn-compression-framework/results/mobilenet_v2.pth.tar",

    "optimizer": {
        "type": "Adam",
        "base_lr": 0.001,
        "schedule_type": "multistep",
        "steps": [20, 30, 40]
    },
    "compression": [
        {
            "algorithm": "rb_sparsity",
            "params": {
                "sparsity_init": 0.01,
                "sparsity_target": 0.52,
                "sparsity_steps": 5,
                "sparsity_training_steps": 10
            },
            "ignored_scopes": ["MobileNetV2/Sequential[features]/Sequential[0]/Conv2d[0]"]
        },
        {
            "algorithm": "quantization",
            "initializer": {
                "num_init_steps": 1
            }
        }
    ]
}
